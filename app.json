{
  "expo": {
    "name": "Online Class Student App",
    "slug": "rn-OnlineClassStudentApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/inteli_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.render.rnonlineclassstudentapp",
      "buildNumber": "1.0.0",
      "supportsTablet": false
    },
    "android": {
      "package": "com.render.rnonlineclassstudentapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/inteli_icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
